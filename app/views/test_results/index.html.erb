
<div class='float-start'>
  <h2>Test results</h2>
</div> 

<div class="clearfix"></div>

 
<table class="table table-hover table-striped">
  <thead>
    <tr class='table-primary'>
      <th>ID </th>
      <th>Result Received </th>
      <th>Patient Name</th>
      <th>Status</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @test_results.each do |test_result| %>
      <tr>
        <td><%= test_result.uuid %> </td>
        <td><%= datetime_format(test_result.created_at) %> </td>
        <td><%= test_result.patient_name %> </td>
        <td><%= test_result.status %></td>
        <td class='text-end'><%= link_to "Details", test_result, class: 'btn btn-sm btn-primary' %> </td>
      </tr>
    <%end%>
  </tbody>
</table>


<%= will_paginate(@test_results) %>